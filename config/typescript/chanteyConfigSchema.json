{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "environment": {
            "type": "string",
            "default": "dom",
            "enum": [
                "dom",
                "core"
            ]
        },
        "title": {
            "type": "string"
        },
        "httpsDev": {
            "type": "boolean"
        }
    },
    "required": [
        "name",
        "environment"
    ]
}
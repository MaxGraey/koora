{
	"name": "koora",
	"version": "0.0.1",
	"description": "AssemblyScript Game Framework",
	"scripts": {
		"build-as": "asc src-as/index.ts --target debug --config ./config/asconfig.json --exportRuntime --enable reference-types",
		"watch-as": "onchange -i \"src-as/**/*\" -- npm run build-as",
		"watch-src-test": "cross-env TS_NODE_PROJECT=\"../../config/webpack/tsconfig.webpack.json\" webpack --watch --config ../../config/webpack/webpack.config.ts",
		"watch-src": "cross-env TS_NODE_PROJECT=\"../../config/webpack/tsconfig.webpack.json\" webpack --watch --config ../../config/webpack/webpack.config.ts --env entry=./src/index.ts",
		"DELETEME-watch-html": "onchange -i -k \"dist/**/*\" -- npm run test-html",
		"start": "live-server ./dist --no-browser",
		"test-html": "live-server --no-browser --watch=./dist,./test",
		"copy-build": "ts-node ./config/scripts/copy-build.ts",
		"watch-build": "onchange -i \"dist/**/*.d.ts\" -- npm run copy-build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mrchantey/koora.git"
	},
	"keywords": [
		"koora",
		"assemblyscript",
		"3d",
		"game"
	],
	"author": "mrchantey",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mrchantey/koora/issues"
	},
	"homepage": "https://github.com/mrchantey/koora#readme",
	"dependencies": {
		"lodash.debounce": "^4.0.8"
	}
}